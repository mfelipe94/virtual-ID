    </Resource>
    <Resource Include="Images\icons8-person-100.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\icons8-security-user-male-100.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
