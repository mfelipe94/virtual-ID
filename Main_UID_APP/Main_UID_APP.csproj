    <UseWPF>true</UseWPF>
    <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\icons8-login-rounded-50.png" />
    <None Remove="Images\icons8-password-key-50.png" />
